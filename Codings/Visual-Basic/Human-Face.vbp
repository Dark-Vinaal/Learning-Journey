Private Sub cmdShow_Click()
'face
Cls
DrawWidth = 5
fillColor = Rnd * 255 * 255 * 255
Circle (5000, 4000), 3000, vbBlue

'Nose
Circle (5000, 4000), 650, vbGreen, -5 * pi / 4, -7 * pi / 4

'Eyes
FillStyle = 0
fillColor = vbWhite
Circle (3500, 3000), 1000, vbBlack, , , 1 / 3
Circle (6500, 3000), 1000, vbBlack, , , 1 / 3

'Eye Ball
DrawWidth = 10
PSet (3500, 3000)
DrawWidth = 10
PSet (6500, 3000)

'Lips
FillStyle = 4
Circle (5000, 6000), 650, vbRed, , , 1 / 3

End Sub
